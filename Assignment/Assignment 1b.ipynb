   "source": [
    "n=int(input(\"Enter a number:- \"))\n",
    "sum=0\n",
    "temp=n\n",
    "length= len(str(n))\n",
    "while(temp>0):\n",
    "    dig=temp%10\n",
    "    sum=sum+(dig**length)\n",
    "    temp=temp//10\n",
    "\n",
    "if(sum==n):\n",
    "    print(\"Armstrong number\")\n",
    "else:\n",
    "    print(\"Not a armstrong number\")"
   ]
